<h1 class="text-center">Add your favorite cocktail!</h1>
<div class="container">
  <br>
  <%= simple_form_for @cocktail do |f| %>
  <div class="form-group">
  <%= f.input :name %>
  </div>
  <div class="form-group">
  <%= f.association :ingredients %>
  </div>
  <div class="form-group">
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  </div>
  <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>
</div>
